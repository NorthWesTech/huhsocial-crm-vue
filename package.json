    "clear:cache:staging": "aws cloudfront create-invalidation --distribution-id=E33Q1FHUK5UMGG --paths=\"/\"",
    "e2e": "cypress open --browser chrome",
    "e2e:silent": "cypress run",
    "lint": "eslint . --ext .ts,.vue,.js",
    "lint:fix": "yarn run eslint --fix"
  },
