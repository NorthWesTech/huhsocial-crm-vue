{
  "name": "wa-influencer-management",
  "version": "1.0.0",
  "description": "influencer management back office web app",
  "author": "Lanistar",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=staging nuxt build",
    "build:staging": "NODE_ENV=staging nuxt build",
    "build:prod": "NODE_ENV=production nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "deploy:assets:dev": "aws s3 cp static s3://assets.dev-lanistar.com/influencers/ --recursive --profile lanistar-development",
    "deploy:prod": "aws s3 sync dist s3://inf-admin.iamlanistar.com/ --delete",
    "clear:cache:prod": "aws cloudfront create-invalidation --distribution-id=E33Q1FHUK5UMGG --paths=\"/\"",
